# Root-level .gitignore

# Node modules (both frontend and backend)
/frontend/node_modules/
/backend/node_modules/

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dependency directories
frontend/.pnp/
backend/.pnp/
.pnp.js

# Build output
frontend/build/
backend/dist/

# Cache directories
frontend/.cache/
backend/.cache/

# Editor and system files
.DS_Store
*.swp
*.swo
*.swn
*.tmp
*.bak

# Environment files
frontend/.env
backend/.env

# Coverage reports
frontend/coverage/
backend/coverage/

# Lint output
frontend/eslintcache
backend/eslintcache
